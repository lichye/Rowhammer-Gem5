//this is atomic version


//__syncthreads ();
            for(int workid=0;workid<num_nodes;workid++){
                if(workid==nid){
                    if(page_rank2[nid]!=newValue)
                        atomicAdd(&page_rank2[nid], addValue);
                }
            }
            
